{
    "name": "cfg-games-support",
    "displayName": "CFG Games Support",
    "description": "Syntax highlighting for .cfg files (Valve games and other games)",
    "version": "0.3.4",
    "author": {
        "name": "JoÃ«l Cancela Vaz & Nassim Bounouas",
        "email": "joel.cancelavaz@gmail.com"
    },
    "publisher": "joelcancela",
    "icon": "icon/icon.png",
    "repository": {
        "url": "https://github.com/joelcancela/VSCode-cfg-support.git",
        "type": "git"
    },
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cfg",
                "aliases": [
                    "Configuration file",
                    "cfg"
                ],
                "extensions": [
                    ".cfg"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cfg",
                "scopeName": "text.source.cfg",
                "path": "./syntaxes/cfg.tmLanguage.json"
            }
        ]
    }
}
